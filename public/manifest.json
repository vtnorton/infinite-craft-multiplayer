{
  "manifest_version": 3,
  "name": "Infinite Craft - Multiplayer extension",
  "description": "This extension allows you to play Infinite Craft with your friends!",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}